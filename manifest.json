{
  "name": "Double Speed",
  "version": "1.0",
  "description": "You will never again have to turn on Double Speed.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
    }
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["main.js"]
    }
  ],
  "manifest_version": 2
}